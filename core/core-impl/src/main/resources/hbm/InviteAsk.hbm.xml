        <property name="inviteUserKey" column="invite_user_key" type="java.lang.Integer"/>
        <property name="inviteUserName" column="invite_user_name" type="java.lang.String"/>
        <property name="driverKey" column="driver_key" type="java.lang.Long"/>
        <property name="driverName" column="driver_name" type="java.lang.String"/>
        <property name="driverPhone" column="driver_phone" type="java.lang.String"/>
        <property name="inviteTime" column="invite_time" type="java.time.LocalDateTime"/>
        <property name="handleTime" column="handle_time" type="java.time.LocalDateTime"/>
        <property name="handleType" column="handle_type" type="java.lang.Integer"/>
    </class>

</hibernate-mapping>